{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "resolveJsonModule": true,
    "declaration": true,
    "outDir": "dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "skipLibCheck": true,
    "paths": {
      // Update line below to target [beta/mainnet] program
      "@suilend/sdk/*": ["../sdk/src/beta/*"],
      // "@suilend/sdk/*": ["../sdk/src/mainnet/*"],
    }
  },
  "include": ["./src/**/*.ts"],
  "exclude": ["node_modules"]
}
