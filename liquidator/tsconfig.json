{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "outDir": "dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "paths": {
      // Update line below to target [beta/mainnet] program
      // "@suilend/sdk/*": ["../sdk/src/beta/*"],
      "@suilend/sdk/*": ["../sdk/src/mainnet/*"]
    }
  },
  "include": ["./src/**/*.ts"],
  "exclude": ["node_modules"]
}
